{
  "name": "hopkins-club-connect",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "NODE_ENV=development yarn prisma:dev && next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "format": "prettier --write \"{prisma,src}/**/*.{ts,tsx}\"",
    "docker:up": "docker compose up --detach --no-recreate --wait",
    "docker:down": "docker compose down --timeout 5",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:seed": "NODE_ENV=development prisma db seed",
    "prisma:prototype": "prisma db push && yarn prisma:seed",
    "prisma:dev": "yarn docker:up && dotenv -e .env -- prisma migrate dev",
    "prisma:dev:studio": "yarn docker:up && dotenv -e .env -- prisma studio",
    "prisma:dev:reset": "yarn docker:up && NODE_ENV=development dotenv -e .env -- prisma migrate reset --force"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.21.3",
    "@prisma/client": "^4.14.0",
